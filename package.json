{
  "name": "sv-homepage",
  "version": "0.1.0",
  "description": "Personal Portfolio",
  "main": "index.js",
  "scripts": {
    "build:dev": "webpack --config ./webpack/dev.config.js",
    "build:prod": "webpack --config ./webpack/prod.config.js",
    "dep-add": "bash ./scripts/dep-add.sh",
    "dep-remove": "bash ./scripts/dep-remove.sh",
    "dep-update": "bash ./scripts/dep-update.sh",
    "dev-client": "webpack-dev-server --config ./webpack/dev.config.js --hot",
    "dev-server": "NODE_ENV=development nodemon",
    "docker-build:dev": "docker-compose run --rm --service-ports build-dev",
    "docker-build:prod": "docker-compose run --rm --service-ports build-prod",
    "docker-dev:hot": "docker-compose -f docker-compose-dev-hot.yml up",
    "docker-image-build-dep": "docker build -t vartanovs/sv-homepage-dependencies -f dev.Dockerfile .",
    "docker-image-remove-dep": "docker image rm vartanovs/sv-homepage-dependencies --force",
    "docker-prune": "docker container prune",
    "docker-remove-containers": "docker rm $(docker ps -a -q -f 'name=sv-homepage') --force",
    "docker-remove-images": "docker rmi $(docker images vartanovs/*sv-homepage* -q) --force",
    "docker-remove-volumes": "docker volume rm $(docker volume ls -q -f name=sv-homepage) --force",
    "docker-remove-all:danger": "docker rm $(docker ps -a -q -f 'name=sv-homepage') --force && docker rmi $(docker images vartanovs/*sv-homepage* -q) --force && docker volume rm $(docker volume ls -q -f name=sv-homepage) --force && exit",
    "docker-stop": "docker stop $(docker ps -a -q --filter status=running)",
    "docker-test": "docker-compose -f docker-compose-test.yml up --abort-on-container-exit",
    "start": "NODE_ENV=production ts-node src/server",
    "test": "NODE_ENV=test jest",
    "test:coverage": "NODE_ENV=test jest --coverage",
    "test:newSnap": "NODE_ENV=development jest --updateSnapshot",
    "test:watch": "NODE_ENV=test jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vartanovs/sv-homepage.git"
  },
  "author": "Serge Vartanov <vartanov.s@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/vartanovs/sv-homepage/issues"
  },
  "homepage": "https://github.com/vartanovs/sv-homepage#readme",
  "dependencies": {
    "express": "^4.16.4",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "ts-node": "^8.0.2",
    "typescript": "^3.2.4"
  },
  "devDependencies": {
    "@types/express": "^4.16.1",
    "@types/node": "^10.12.19",
    "@types/react": "^16.7.22",
    "@types/react-dom": "^16.0.11",
    "css-loader": "^2.1.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^23.6.0",
    "nodemon": "^1.18.9",
    "style-loader": "^0.23.1",
    "supertest": "^3.4.2",
    "ts-jest": "^23.10.5",
    "ts-loader": "^5.3.3",
    "tslint": "^5.12.1",
    "tslint-config-airbnb": "^5.11.1",
    "tslint-config-prettier": "^1.17.0",
    "webpack": "^4.29.0",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.1.14",
    "webpack-merge": "^4.2.1"
  }
}
